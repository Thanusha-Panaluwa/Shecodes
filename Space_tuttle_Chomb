#Turtle graphics gamr - Space Turtle
import turtle

#Set up screen
turtle.setup(650,650)
wn = turtle.Screen()
wn.bgcolor('navy')

#Create player turtle
player = turtle.Turtle()
player.color('darkorange')
player.shape('turtle')
player.penup()

#setup speed variable
speed = 1
Speed = 2

while True:
    player.forward(speed)
  
